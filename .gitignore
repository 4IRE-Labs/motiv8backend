# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history

# Ignore Docker data
postgres-data/
redis-data/

# Ignoring spec coverage report
coverage
# Ignore application configuration
/config/application.yml
# Ignore RubyMine/Idea files
.idea/
# Ignove Visual Studio Code
.vscode
# Ignore Mac OS Files
.DS_Store
# Ignore DockStation files
.env

/config/database.yml

# Ignore venv
eth/venv/*
